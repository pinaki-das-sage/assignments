{% extends "layout.html.j2" %}

{% block title %} Assignment 9 {% endblock %}

{% block body %}
    <div class="chart" id="barchart">
    </div>

    <b>Accuracy of predictions: </b>{{ values.accuracy_value | safe }} <br/>
    <b>Baseline accuracy: </b>{{ values.baseline_accuracy | safe }}<br/>
    <b>Accuracy improvement: </b>{{ values.accuracy_improvement | safe }}<br/>
{% endblock %}

{% block script %}
    <script>
        let graphs = {{graphJSON | safe}};
        Plotly.plot('barchart', graphs, {});
    </script>
{% endblock %}